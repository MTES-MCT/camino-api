{
  "name": "camino-api",
  "version": "0.14.0",
  "description": "Camino api",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MTES-MCT/camino-api.git"
  },
  "author": {
    "name": "Fran√ßois Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "homepage": "https://github.com/MTES-MCT/camino-api",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "dev": "nodemon",
    "import": "rm -rf sources && mkdir sources && node ./dist/tools/import/index.js",
    "dev:import": "ts-node --transpile-only ./src/tools/import/index.js",
    "export": "node ./dist/tools/export/index.js",
    "dev:export": "ts-node --transpile-only ./src/tools/export/index.js",
    "export:users": "node ./dist/tools/export/utilisateurs.js",
    "export:activites": "node ./dist/tools/export/titres-activites.js",
    "migrate:rollback": "npx knex --knexfile=./knex/config-cli.js migrate:rollback",
    "migrate:latest": "npx knex --knexfile=./knex/config-cli.js migrate:latest",
    "seed:run": "npx npx knex --knexfile=./knex/config-cli.js seed:run",
    "migrate": "npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "migrate-api": "node ./knex/recreate.js && node ./knex/migrate.js",
    "daily": "node ./dist/tasks/daily.js",
    "dev:daily": "ts-node --transpile-only ./src/tasks/daily.ts",
    "monthly": "node ./dist/tasks/monthly.js",
    "dev:monthly": "ts-node --transpile-only ./src/tasks/monthly.ts",
    "start": "npm run build && node ./dist/index.js",
    "graphql:schema": "node ./tools/utils/graphql-dsl-to-query-convert.js",
    "test": "jest --verbose && codecov",
    "jest": "jest --verbose --coverage --watch",
    "commit": "npx git-cz",
    "release": "standard-version && git push --follow-tags origin master && npx conventional-github-releaser",
    "doc": "rm -rf docs && npx esdoc",
    "user-add": "ts-node ./src/tools/dev/database/utilisateur-admin-add.js"
  },
  "dependencies": {
    "@mapbox/geojsonhint": "^3.0.0",
    "@sentry/node": "^5.0.8",
    "@sentry/types": "^5.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/crypto-random-string": "^2.0.0",
    "@types/dateformat": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/express-jwt": "0.0.42",
    "@types/geojson": "^7946.0.7",
    "@types/graphql": "^14.2.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.13.4",
    "@types/node-fetch": "^2.3.2",
    "bcrypt": "^3.0.6",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "crypto-random-string": "^2.0.0",
    "dateformat": "^3.0.3",
    "decamelize": "^3.2.0",
    "dotenv": "^7.0.0",
    "email-regex": "^3.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "express-jwt": "^5.3.1",
    "fs-extra": "^7.0.1",
    "geojson-rewind": "^0.3.1",
    "googleapis": "^39.2.0",
    "graphql": "^14.2.1",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.16.5",
    "knex-db-manager": "^0.5.0",
    "make-dir": "^3.0.0",
    "node-fetch": "^2.3.0",
    "nodemailer": "^6.1.0",
    "nodemailer-html-to-text": "^3.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "objection": "^1.6.7",
    "p-queue": "^5.0.0",
    "pg": "^7.10.0",
    "pg-escape": "^0.2.0",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@typescript-eslint/parser": "^1.6.0",
    "codecov": "^3.3.0",
    "commitizen": "^3.0.7",
    "conventional-github-releaser": "^3.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsdoc": "^4.8.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "sql-formatter": "^2.3.2",
    "standard-version": "^5.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "prettier",
      "jest",
      "jsdoc"
    ],
    "rules": {
      "jsdoc/check-examples": 1,
      "jsdoc/check-param-names": 1,
      "jsdoc/check-tag-names": 1,
      "jsdoc/check-types": 1,
      "jsdoc/newline-after-description": 1,
      "jsdoc/no-undefined-types": 1,
      "jsdoc/require-description": 1,
      "jsdoc/require-description-complete-sentence": 1,
      "jsdoc/require-example": 1,
      "jsdoc/require-hyphen-before-param-description": 1,
      "jsdoc/require-param": 1,
      "jsdoc/require-param-description": 1,
      "jsdoc/require-param-name": 1,
      "jsdoc/require-param-type": 1,
      "jsdoc/require-returns": 1,
      "jsdoc/require-returns-description": 1,
      "jsdoc/require-returns-type": 1,
      "jsdoc/valid-types": 1,
      "no-throw-literal": 0,
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "manual": {
            "index": "./manual/infos.md",
            "asset": "./manual/asset",
            "files": [
              "./manual/docker.md"
            ]
          }
        }
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "objectRestSpread": true,
          "classProperties": true
        }
      }
    ]
  }
}
