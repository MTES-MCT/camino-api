{
  "name": "camino-api",
  "version": "0.24.17",
  "description": "Camino api",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/MTES-MCT/camino-api"
  },
  "author": {
    "name": "Fran√ßois Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "homepage": "https://github.com/MTES-MCT/camino-api",
  "main": "index.js",
  "scripts": {
    "bim": "npm i && npm run build && npm run db:recreate && npm run db:import",
    "build": "rm -rf dist && tsc && cp -r ./src/api/schemas ./dist/api",
    "commit": "git-cz",
    "daily": "node ./dist/business/daily.js",
    "db:dump": "pg_dump --clean --dbname=camino --username=postgres --host=localhost --file=./backups/camino.sql",
    "db:dump-public": "node ./knex/copy-clean.js && pg_dump --clean --dbname=camino_public --username=postgres --host=localhost --file=./backups/camino-public.sql && dropdb --username=postgres --host=localhost camino_public",
    "db:import": "psql --dbname=camino --username=postgres --host=localhost < ./backups/camino.sql",
    "db:import-public": "psql --dbname=camino --username=postgres --host=localhost < ./backups/camino-public.sql",
    "db:init": "node ./knex/recreate.js && node ./knex/migrate.js",
    "db:rollback": "knex --knexfile ./knex/config-cli.js migrate:rollback",
    "db:migrate": "knex --knexfile ./knex/config-cli.js migrate:latest",
    "db:seed": "knex --knexfile ./knex/config-cli.js seed:run",
    "db:recreate": "dropdb --username=postgres --host=localhost camino && createdb --username=postgres --host=localhost camino",
    "db:prod-fetch": "scp $u@camino.beta.gouv.fr:/srv/backups/camino.sql backups/",
    "db:test-push": "npm run db:dump-public && scp backups/camino-public.sql $u@test.camino.beta.gouv.fr:/srv/backups/camino.sql",
    "db:user": "node ./knex/user-add.js",
    "dev": "nodemon",
    "dev:daily": "ts-node --transpile-only ./src/business/daily.ts",
    "dev:daily-debug": "node -r ts-node/register --inspect-brk=8080 ./src/business/daily.ts",
    "dev:export": "ts-node --transpile-only ./src/tools/export/index.js",
    "dev:import": "ts-node --transpile-only ./src/tools/import/index.js",
    "dev:import-check": "ts-node --transpile-only ./src/tools/dev/json/import-check.js",
    "dev:documents-check": "ts-node --transpile-only ./src/tools/dev/database/documents-check.js",
    "dev:monthly": "ts-node --transpile-only ./src/business/monthly.ts",
    "dev:schema": "ts-node ./src/tools/dev/api/graphql-dsl-to-query-convert.js",
    "dev:update": "npm-check-updates -u && npm install && npm audit fix",
    "doc": "rm -rf docs && esdoc",
    "monthly": "node ./dist/business/monthly.js",
    "release": "standard-version && git push --follow-tags upstream master && conventional-github-releaser",
    "ss:export": "node ./dist/tools/export/index.js",
    "ss:users-export": "node ./dist/tools/export/utilisateurs.js",
    "ss:activites-export": "node ./dist/tools/export/titres-activites.js",
    "ss:import": "rm -rf sources && mkdir sources && node ./dist/tools/import/index.js",
    "start": "node ./dist/index.js",
    "test": "eslint src/ && eslint knex/ && tslint 'src/**/*.ts' && jest --verbose && codecov",
    "test:lint": "eslint --fix src/ && eslint --fix knex/ && tslint --fix 'src/**/*.ts'",
    "test:jest": "npm run test:lint && jest --verbose --watch",
    "test:cypress": "npm run db:import-public && npm run daily && npm run db:user  && npm run dev"
  },
  "dependencies": {
    "@mapbox/geojsonhint": "^3.0.0",
    "@sentry/node": "^5.11.1",
    "@sentry/types": "^5.11.0",
    "@sindresorhus/slugify": "^0.10.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/geojson": "^7946.0.7",
    "@types/graphql-upload": "^8.0.3",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/node": "^13.1.8",
    "@types/node-fetch": "^2.5.4",
    "@types/proj4": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "camelcase": "^5.3.1",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "crypto-random-string": "^3.1.0",
    "dateformat": "^3.0.3",
    "decamelize": "^3.2.0",
    "dotenv": "^8.2.0",
    "email-regex": "^4.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^5.3.1",
    "geojson-rewind": "^0.3.1",
    "googleapis": "^47.0.0",
    "graphql": "^14.5.8",
    "graphql-fields": "^2.0.3",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-upload": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.20.8",
    "knex-db-manager": "^0.5.0",
    "make-dir": "^3.0.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "nodemailer-html-to-text": "^3.1.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-check-updates": "^4.0.1",
    "objection": "^2.1.2",
    "p-queue": "^6.2.1",
    "pg": "^7.17.1",
    "pg-escape": "^0.2.0",
    "proj4": "^2.6.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@typescript-eslint/parser": "^2.17.0",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "^3.0.2",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsdoc": "^20.3.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier-eslint": "^9.0.1",
    "prettier-tslint": "^0.4.2",
    "sql-formatter": "^2.3.3",
    "standard-version": "^7.1.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "prettier",
      "jest",
      "jsdoc"
    ],
    "rules": {
      "newline-before-return": 1,
      "jsdoc/check-examples": 1,
      "jsdoc/check-param-names": 1,
      "jsdoc/check-tag-names": 1,
      "jsdoc/check-types": 1,
      "jsdoc/newline-after-description": 1,
      "jsdoc/no-undefined-types": 1,
      "jsdoc/require-description": 1,
      "jsdoc/require-description-complete-sentence": 1,
      "jsdoc/require-hyphen-before-param-description": 1,
      "jsdoc/require-param": 1,
      "jsdoc/require-param-description": 1,
      "jsdoc/require-param-name": 1,
      "jsdoc/require-param-type": 1,
      "jsdoc/require-returns": 1,
      "jsdoc/require-returns-description": 1,
      "jsdoc/require-returns-type": 1,
      "jsdoc/valid-types": 1,
      "no-throw-literal": 0,
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "manual": {
            "index": "./manual/infos.md",
            "asset": "./manual/asset",
            "files": [
              "./manual/docker.md"
            ]
          }
        }
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "objectRestSpread": true,
          "classProperties": true
        }
      }
    ]
  }
}
