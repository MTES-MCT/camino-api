{
  "name": "camino-api",
  "version": "0.24.43",
  "description": "Camino api",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/MTES-MCT/camino-api"
  },
  "author": {
    "name": "François Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "homepage": "https://github.com/MTES-MCT/camino-api",
  "main": "index.js",
  "scripts": {
    "bim": "npm i && npm run build && npm run db:recreate && npm run db:import",
    "build": "rm -rf dist && tsc && mkdir -p ./dist/api/graphql && cp -r ./src/api/graphql/schemas ./dist/api/graphql",
    "commit": "git-cz",
    "daily": "node ./dist/business/daily.js",
    "db:dump": "pg_dump --clean --if-exists --format c --no-owner --no-privileges --dbname=camino --file=./backups/camino.sql",
    "db:import": "pg_restore --clean --if-exists --no-owner --no-privileges --dbname=camino < ./backups/camino.sql",
    "db:init": "node ./knex/recreate.js && node ./knex/migrate.js",
    "db:migrate": "knex --knexfile ./knex/config.js migrate:latest",
    "db:prod-fetch": "scp $u@camino.beta.gouv.fr:/srv/backups/camino.sql backups/",
    "db:public-dump": "node ./knex/public-copy.js && pg_dump --clean --if-exists --format c --no-owner --no-privileges --dbname=camino_public --file=./backups/camino-public.sql && dropdb camino_public",
    "db:public-import": "pg_restore --clean --if-exists --no-owner --no-privileges --dbname=camino < ./backups/camino-public.sql",
    "db:recreate": "dropdb camino && createdb camino",
    "db:rollback": "knex --knexfile ./knex/config.js migrate:rollback",
    "db:seed": "knex --knexfile ./knex/config.js seed:run",
    "db:test-push": "scp backups/camino-public.sql $u@test.camino.beta.gouv.fr:/srv/backups/camino.sql",
    "db:user": "node ./knex/user-add-cli.js",
    "dev": "nodemon",
    "dev:daily": "ts-node --transpile-only ./src/business/daily.ts",
    "dev:daily-debug": "node -r ts-node/register --inspect-brk=8080 ./src/business/daily.ts",
    "dev:backups-archive": "tar -zcvf backups/`date +%Y%m%d_%H%M%S`-camino.tar.gz backups/files/* backups/camino.sql",
    "dev:export": "ts-node --transpile-only ./src/tools/export/index.js",
    "dev:import": "ts-node --transpile-only ./src/tools/import/index.ts",
    "dev:import-check": "ts-node --transpile-only ./src/tools/dev/json/import-check.js",
    "dev:documents-check": "ts-node --transpile-only ./dev/database/documents-check.ts",
    "dev:monthly": "ts-node --transpile-only ./src/business/monthly.ts",
    "dev:schema": "node dev/api/graphql-dsl-to-query-convert.js",
    "dev:update": "npm-check-updates -u && npm install && npm audit fix",
    "docs": "rm -rf docs && typedoc src --sourcefile-url-prefix \"https://github.com/MTES-MCT/camino-api/blob/master/\"",
    "lint": "eslint --fix .",
    "monthly": "node ./dist/business/monthly.js",
    "release": "standard-version && git push --follow-tags upstream master && conventional-github-releaser",
    "ss:export": "node ./dist/tools/export/index.js",
    "ss:users-export": "node ./dist/tools/export/utilisateurs.js",
    "ss:activites-export": "node ./dist/tools/export/titres-activites.js",
    "ss:import": "rm -rf sources && mkdir sources && node ./dist/tools/import/index.js",
    "start": "node ./dist/index.js",
    "test": "eslint . && npm run build && jest --testPathIgnorePatterns=tests --ci && codecov",
    "test:jest": "npm run lint && jest --testPathIgnorePatterns=tests --watch",
    "test:cypress": "npm run db:public-import && npm run daily && npm run db:user && npm run dev",
    "test:integration": "TESTS_INTEGRATION=true PGDATABASE=camino_tests JWT_SECRET=secret-tests jest --testPathIgnorePatterns=src --runInBand --detectOpenHandles --watch",
    "ci:lint": "eslint .",
    "ci:test-unit": "jest --testPathIgnorePatterns=tests --ci",
    "ci:test-integration": "TESTS_INTEGRATION=true PGDATABASE=camino_tests JWT_SECRET=secret-tests jest --testPathIgnorePatterns=src --runInBand --detectOpenHandles --ci --forceExit",
    "ci:codecov": "codecov"
  },
  "@comment dependencies": {
    "graphql-import": "bloqué à 0.7.1: https://github.com/ardatan/graphql-import/issues/537",
    "graphql": "bloqué à 14.6.0 à cause d'une erreur de type de graphql-fields",
    "knex-db-manager": "en attente de la prochaine release qui contient la migration séquentielle"
  },
  "dependencies": {
    "@mapbox/geojsonhint": "^3.0.0",
    "@sentry/node": "^5.17.0",
    "@sentry/types": "^5.17.0",
    "@sindresorhus/slugify": "^1.0.0",
    "@types/basic-auth": "^1.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.6",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/geojson": "^7946.0.7",
    "@types/graphql-fields": "^1.3.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/graphql-upload": "^8.0.3",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/proj4": "^2.5.0",
    "@types/sql-formatter": "^2.3.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "camelcase": "^6.0.0",
    "cerbere": "^0.2.1",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto-random-string": "^3.2.0",
    "dateformat": "^3.0.3",
    "decamelize": "^4.0.0",
    "dotenv": "^8.2.0",
    "email-regex": "^4.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^5.3.3",
    "geojson-rewind": "^0.3.1",
    "googleapis": "^52.1.0",
    "graphql": "14.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-import": "0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "knex-db-manager": "^0.6.1",
    "make-dir": "^3.1.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.10",
    "nodemailer-html-to-text": "^3.1.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-check-updates": "^7.0.1",
    "objection": "^2.2.0",
    "p-queue": "^6.4.0",
    "pg": "^8.2.1",
    "proj4": "^2.6.2",
    "typescript": "^3.9.5",
    "xlsx": "^0.16.2"
  },
  "devDependencies": {
    "@types/knex-db-manager": "^0.6.0",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "codecov": "^3.7.0",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tsdoc": "^0.2.5",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "pg-escape": "^0.2.0",
    "prettier-eslint": "^11.0.0",
    "sql-formatter": "^2.3.3",
    "standard-version": "^8.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-pages": "^1.0.1",
    "typedoc-plugin-sourcefile-url": "^1.0.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended",
      "standard",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/standard"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "prettier",
      "jest",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/camelcase": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-non-null-assertion": 0,
      "@typescript-eslint/no-use-before-define": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/interface-name-prefix": 0,
      "newline-before-return": 1
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
