{
  "name": "camino-api",
  "version": "0.24.57",
  "description": "Camino api",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/MTES-MCT/camino-api"
  },
  "author": {
    "name": "François Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "homepage": "https://github.com/MTES-MCT/camino-api",
  "main": "index.js",
  "scripts": {
    "bim": "npm i && npm run build && npm run db:recreate && npm run db:import",
    "build": "rm -rf dist && tsc && mkdir -p ./dist/api/graphql && cp -r ./src/api/graphql/schemas ./dist/api/graphql",
    "commit": "git-cz",
    "daily": "node ./dist/scripts/daily.js",
    "monthly": "node ./dist/scripts/monthly.js",
    "db:dump": "pg_dump --host=localhost --username=postgres --clean --if-exists --format=c --no-owner --no-privileges --dbname=camino --file=./backups/camino.sql",
    "db:import": "pg_restore ./backups/camino.sql --host=localhost --username=postgres --clean --if-exists --no-owner --no-privileges --dbname=camino",
    "db:init": "node ./knex/recreate.js && node ./knex/migrate.js",
    "db:migrate": "knex --knexfile ./knex/config.js migrate:latest",
    "db:prod-fetch": "scp $u@camino.beta.gouv.fr:/srv/backups/camino.sql backups/",
    "db:public-dump": "node ./knex/public-copy.js && pg_dump --clean --if-exists --host=localhost --username=postgres --format=c --no-owner --no-privileges --dbname=camino_public --file=./backups/camino-public.sql && dropdb --host=localhost --username=postgres camino_public",
    "db:public-import": "pg_restore ./backups/camino-public.sql --host=localhost --username=postgres --clean --if-exists --no-owner --no-privileges --dbname=camino",
    "db:recreate": "dropdb --host=localhost --username=postgres camino && createdb --host=localhost --username=postgres camino",
    "db:rollback": "knex --knexfile ./knex/config.js migrate:rollback",
    "db:seed": "knex --knexfile ./knex/config.js seed:run",
    "db:test-push": "scp backups/camino-public.sql $u@test.camino.beta.gouv.fr:/srv/backups/camino.sql",
    "db:user": "node ./knex/user-add-cli.js",
    "dev": "nodemon",
    "dev:daily": "ts-node --transpile-only ./src/scripts/daily.ts",
    "dev:daily-debug": "node -r ts-node/register --inspect-brk=8080 ./src/scripts/daily.ts",
    "dev:backups-archive": "tar -zcvf backups/`date +%Y%m%d_%H%M%S`-camino.tar.gz backups/files/* backups/camino.sql",
    "dev:export": "ts-node --transpile-only ./src/scripts/ss-export.ts",
    "dev:import": "ts-node --transpile-only ./src/scripts/ss-import.ts",
    "dev:monthly": "ts-node --transpile-only ./src/scripts/monthly.ts",
    "dev:schema": "node dev/api/graphql-dsl-to-query-convert.js",
    "dev:update": "npm-check-updates -u && npm install && npm audit fix",
    "docs": "rm -rf docs && typedoc src --sourcefile-url-prefix \"https://github.com/MTES-MCT/camino-api/blob/master/\"",
    "documents:check": "node ./dist/scripts/documents-check.js",
    "lint": "eslint --fix .",
    "release": "standard-version && git push --follow-tags upstream master && conventional-github-releaser",
    "ss:export": "node ./dist/scripts/ss-export.js",
    "ss:import": "rm -rf sources && mkdir sources && node ./dist/scripts/ss-import.js",
    "start": "node ./dist/index.js & node ./dist/cron.js",
    "test": "eslint . && npm run build && jest --testPathIgnorePatterns=tests",
    "test:unit": "npm run lint && jest --testPathIgnorePatterns=tests --watch",
    "test:cypress": "npm run db:public-import && npm run daily && npm run db:user && npm run dev",
    "test:integration": "TESTS_INTEGRATION=true PGDATABASE=camino_tests JWT_SECRET=secret-tests jest --testPathIgnorePatterns=src --runInBand --detectOpenHandles --watch --testTimeout=20000",
    "ci:lint": "eslint .",
    "ci:test-unit": "jest --testPathIgnorePatterns=tests --ci",
    "ci:test-integration": "TESTS_INTEGRATION=true PGDATABASE=camino_tests JWT_SECRET=secret-tests JWT_SECRET_REFRESH=refresh-secret jest --testPathIgnorePatterns=src --runInBand --detectOpenHandles --ci --forceExit --testTimeout=20000",
    "ci:codecov": "codecov"
  },
  "@comment dependencies": {
    "graphql-import": "bloqué à 0.7.1: https://github.com/MTES-MCT/camino-api/issues/399 & https://github.com/MTES-MCT/camino-api/issues/424"
  },
  "dependencies": {
    "@mapbox/geojsonhint": "^3.0.0",
    "@sentry/node": "5.27.2",
    "@sentry/types": "5.27.2",
    "@sindresorhus/slugify": "^1.1.0",
    "@turf/center": "^6.0.1",
    "@types/basic-auth": "^1.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.8",
    "@types/cron": "^1.7.2",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/geojson": "^7946.0.7",
    "@types/graphql-fields": "^1.3.3",
    "@types/graphql-type-json": "^0.3.2",
    "@types/graphql-upload": "^8.0.4",
    "@types/jest": "26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/proj4": "^2.5.0",
    "@types/sql-formatter": "^2.3.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "camelcase": "^6.2.0",
    "cerbere": "^0.2.1",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.2",
    "crypto-random-string": "^3.3.0",
    "dateformat": "^3.0.3",
    "decamelize": "^4.0.0",
    "dotenv": "^8.2.0",
    "email-regex": "^4.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-jwt": "^6.0.0",
    "geojson-rewind": "^0.3.1",
    "googleapis": "^63.0.0",
    "graphql": "15.4.0",
    "graphql-fields": "^2.0.3",
    "graphql-import": "0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "knex-db-manager": "^0.7.0",
    "make-dir": "^3.1.0",
    "matomo-tracker": "^2.2.4",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.14",
    "nodemailer-html-to-text": "^3.1.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "npm-check-updates": "^9.2.4",
    "objection": "^2.2.3",
    "p-queue": "^6.6.2",
    "pg": "^8.4.2",
    "proj4": "^2.6.2",
    "typescript": "^4.0.5",
    "winston": "^3.3.3",
    "xlsx": "^0.16.8"
  },
  "devDependencies": {
    "@types/knex-db-manager": "^0.6.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "conventional-github-releaser": "^0.5.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-tsdoc": "^0.2.7",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "pg-escape": "^0.2.0",
    "prettier-eslint": "^11.0.0",
    "sql-formatter": "^2.3.3",
    "standard-version": "^9.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-pages": "^1.0.1",
    "typedoc-plugin-sourcefile-url": "^1.0.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended",
      "standard",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/standard"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "prettier",
      "jest",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-empty-interface": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-non-null-assertion": 0,
      "@typescript-eslint/no-var-requires": 0,
      "newline-before-return": 1,
      "no-use-before-define": 0,
      "no-redeclare": 0,
      "no-return-await": "error",
      "default-param-last": 0
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
