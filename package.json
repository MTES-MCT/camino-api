{
  "name": "camino-api",
  "version": "0.12.1",
  "description": "Camino api",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MTES-MCT/camino-api.git"
  },
  "author": {
    "name": "Fran√ßois Romain",
    "email": "francoisromain@gmail.com",
    "url": "https://francoisromain.com"
  },
  "homepage": "https://github.com/MTES-MCT/camino-api",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon --watch 'src/**' --exec 'ts-node' src/index.ts",
    "import": "mkdir -p sources && node ./dist/tools/import/index.js",
    "export": "node ./dist/tools/export/index.js",
    "export:users": "node ./dist/tools/export/utilisateurs.js",
    "export:rapports": "node ./dist/tools/export/titres-travaux-rapports.js",
    "migrate:rollback": "npx knex --knexfile=./knex/config-cli.js migrate:rollback",
    "migrate:latest": "npx knex --knexfile=./knex/config-cli.js migrate:latest",
    "seed:run": "npx npx knex --knexfile=./knex/config-cli.js seed:run",
    "migrate": "npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "migrate-api": "node ./knex/index.js",
    "daily": "node ./dist/tasks/daily.js",
    "start": "npm run build && node ./dist/index.js",
    "graphql:schema": "node ./tools/utils/graphql-dsl-to-query-convert.js",
    "test": "jest --verbose && codecov",
    "jest": "jest --verbose --coverage --watch",
    "commit": "npx git-cz",
    "release": "standard-version && git push --follow-tags origin master && conventional-github-releaser",
    "doc": "rm -rf docs && npx esdoc",
    "user-add": "ts-node ./src/tools/dev/database/utilisateur-admin-add.js"
  },
  "dependencies": {
    "@sentry/node": "^4.6.4",
    "@sentry/types": "^4.5.3",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/crypto-random-string": "^1.0.0",
    "@types/dateformat": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/express-jwt": "0.0.41",
    "@types/geojson": "^7946.0.6",
    "@types/graphql": "^14.0.7",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/knex": "^0.15.2",
    "@types/node": "^11.9.5",
    "@types/node-fetch": "^2.1.6",
    "bcrypt": "^3.0.4",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "crypto-random-string": "^1.0.0",
    "dateformat": "^3.0.3",
    "decamelize": "^2.0.0",
    "dotenv": "^6.2.0",
    "email-regex": "^3.0.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-jwt": "^5.3.1",
    "fs-extra": "^7.0.1",
    "geojson-rewind": "^0.3.1",
    "google-spreadsheet": "^2.0.7",
    "google-spreadsheet-to-json": "^1.0.0",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.0",
    "knex": "0.16.3",
    "node-fetch": "^2.3.0",
    "nodemailer": "^5.1.1",
    "nodemailer-html-to-text": "^3.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "objection": "^1.6.3",
    "p-queue": "^3.1.0",
    "pg": "^7.8.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "^2.1.0",
    "@types/jest": "^24.0.9",
    "@typescript-eslint/parser": "^1.4.2",
    "codecov": "^3.2.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsdoc": "^4.1.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "standard-version": "^5.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "env": {
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "jest",
      "jsdoc"
    ],
    "rules": {
      "jsdoc/check-examples": 1,
      "jsdoc/check-param-names": 1,
      "jsdoc/check-tag-names": 1,
      "jsdoc/check-types": 1,
      "jsdoc/newline-after-description": 1,
      "jsdoc/no-undefined-types": 1,
      "jsdoc/require-description": 1,
      "jsdoc/require-description-complete-sentence": 1,
      "jsdoc/require-example": 1,
      "jsdoc/require-hyphen-before-param-description": 1,
      "jsdoc/require-param": 1,
      "jsdoc/require-param-description": 1,
      "jsdoc/require-param-name": 1,
      "jsdoc/require-param-type": 1,
      "jsdoc/require-returns": 1,
      "jsdoc/require-returns-description": 1,
      "jsdoc/require-returns-type": 1,
      "jsdoc/valid-types": 1
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "manual": {
            "index": "./manual/infos.md",
            "asset": "./manual/asset",
            "files": [
              "./manual/docker.md"
            ]
          }
        }
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "objectRestSpread": true,
          "classProperties": true
        }
      }
    ]
  }
}
